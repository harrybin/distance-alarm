<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!-- Material Design 3.0 Color Tokens -->
    
    <!-- Primary Colors -->
    <Color x:Key="Primary">#1976D2</Color>
    <Color x:Key="PrimaryContainer">#BBDEFB</Color>
    <Color x:Key="OnPrimary">#FFFFFF</Color>
    <Color x:Key="OnPrimaryContainer">#0D47A1</Color>
    
    <!-- Primary Dark Theme -->
    <Color x:Key="PrimaryDark">#90CAF9</Color>
    <Color x:Key="PrimaryContainerDark">#0D47A1</Color>
    <Color x:Key="OnPrimaryDark">#0D47A1</Color>
    <Color x:Key="OnPrimaryContainerDark">#E3F2FD</Color>
    
    <!-- Secondary Colors -->
    <Color x:Key="Secondary">#1565C0</Color>
    <Color x:Key="SecondaryContainer">#E1F5FE</Color>
    <Color x:Key="OnSecondary">#FFFFFF</Color>
    <Color x:Key="OnSecondaryContainer">#0277BD</Color>
    
    <!-- Secondary Dark Theme -->
    <Color x:Key="SecondaryDark">#81D4FA</Color>
    <Color x:Key="SecondaryContainerDark">#0277BD</Color>
    <Color x:Key="OnSecondaryDark">#0277BD</Color>
    <Color x:Key="OnSecondaryContainerDark">#E0F7FA</Color>
    
    <!-- Tertiary Colors -->
    <Color x:Key="Tertiary">#00BCD4</Color>
    <Color x:Key="TertiaryContainer">#E0F2F1</Color>
    <Color x:Key="OnTertiary">#FFFFFF</Color>
    <Color x:Key="OnTertiaryContainer">#00695C</Color>
    
    <!-- Error Colors -->
    <Color x:Key="Error">#D32F2F</Color>
    <Color x:Key="ErrorContainer">#FFEBEE</Color>
    <Color x:Key="OnError">#FFFFFF</Color>
    <Color x:Key="OnErrorContainer">#B71C1C</Color>
    
    <!-- Surface Colors -->
    <Color x:Key="Surface">#FAFAFA</Color>
    <Color x:Key="SurfaceVariant">#F5F5F5</Color>
    <Color x:Key="OnSurface">#212121</Color>
    <Color x:Key="OnSurfaceVariant">#424242</Color>
    
    <!-- Surface Dark Theme -->
    <Color x:Key="SurfaceDark">#121212</Color>
    <Color x:Key="SurfaceVariantDark">#1E1E1E</Color>
    <Color x:Key="OnSurfaceDark">#E0E0E0</Color>
    <Color x:Key="OnSurfaceVariantDark">#BDBDBD</Color>
    
    <!-- Background Colors -->
    <Color x:Key="Background">#FFFFFF</Color>
    <Color x:Key="OnBackground">#212121</Color>
    <Color x:Key="BackgroundDark">#000000</Color>
    <Color x:Key="OnBackgroundDark">#E0E0E0</Color>
    
    <!-- Outline Colors -->
    <Color x:Key="Outline">#9E9E9E</Color>
    <Color x:Key="OutlineVariant">#E0E0E0</Color>
    <Color x:Key="OutlineDark">#616161</Color>
    <Color x:Key="OutlineVariantDark">#424242</Color>
    
    <!-- Legacy Colors for Compatibility -->
    <Color x:Key="White">#FFFFFF</Color>
    <Color x:Key="Black">#000000</Color>
    <Color x:Key="Gray100">#F5F5F5</Color>
    <Color x:Key="Gray200">#EEEEEE</Color>
    <Color x:Key="Gray300">#E0E0E0</Color>
    <Color x:Key="Gray400">#BDBDBD</Color>
    <Color x:Key="Gray500">#9E9E9E</Color>
    <Color x:Key="Gray600">#757575</Color>
    <Color x:Key="Gray700">#616161</Color>
    <Color x:Key="Gray800">#424242</Color>
    <Color x:Key="Gray900">#212121</Color>
    <Color x:Key="Gray950">#121212</Color>

    <!-- Material Design Brushes -->
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"/>
    <SolidColorBrush x:Key="PrimaryContainerBrush" Color="{StaticResource PrimaryContainer}"/>
    <SolidColorBrush x:Key="OnPrimaryBrush" Color="{StaticResource OnPrimary}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource Secondary}"/>
    <SolidColorBrush x:Key="SecondaryContainerBrush" Color="{StaticResource SecondaryContainer}"/>
    <SolidColorBrush x:Key="OnSecondaryBrush" Color="{StaticResource OnSecondary}"/>
    <SolidColorBrush x:Key="TertiaryBrush" Color="{StaticResource Tertiary}"/>
    <SolidColorBrush x:Key="ErrorBrush" Color="{StaticResource Error}"/>
    <SolidColorBrush x:Key="SurfaceBrush" Color="{StaticResource Surface}"/>
    <SolidColorBrush x:Key="SurfaceVariantBrush" Color="{StaticResource SurfaceVariant}"/>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource Background}"/>
    <SolidColorBrush x:Key="OnSurfaceBrush" Color="{StaticResource OnSurface}"/>
    <SolidColorBrush x:Key="OnBackgroundBrush" Color="{StaticResource OnBackground}"/>
    <SolidColorBrush x:Key="OutlineBrush" Color="{StaticResource Outline}"/>
    
    <!-- Legacy Brushes for Compatibility -->
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}"/>
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource Black}"/>
    <SolidColorBrush x:Key="Gray100Brush" Color="{StaticResource Gray100}"/>
    <SolidColorBrush x:Key="Gray200Brush" Color="{StaticResource Gray200}"/>
    <SolidColorBrush x:Key="Gray300Brush" Color="{StaticResource Gray300}"/>
    <SolidColorBrush x:Key="Gray400Brush" Color="{StaticResource Gray400}"/>
    <SolidColorBrush x:Key="Gray500Brush" Color="{StaticResource Gray500}"/>
    <SolidColorBrush x:Key="Gray600Brush" Color="{StaticResource Gray600}"/>
    <SolidColorBrush x:Key="Gray700Brush" Color="{StaticResource Gray700}"/>
    <SolidColorBrush x:Key="Gray800Brush" Color="{StaticResource Gray800}"/>
    <SolidColorBrush x:Key="Gray900Brush" Color="{StaticResource Gray900}"/>
    <SolidColorBrush x:Key="Gray950Brush" Color="{StaticResource Gray950}"/>
</ResourceDictionary>